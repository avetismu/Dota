<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout_news"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tesla.dota.Activity.News">

    <FrameLayout
        android:id="@+id/news_container"
        android:layout_marginTop="@dimen/ui_margin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


    <fragment android:id="@+id/navigation_drawer_news"
        android:layout_width="@dimen/navigation_drawer_width"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="com.tesla.dota.Fragment.NavigationDrawerFragment"
        tools:layout="@layout/fragment_navigation_drawer" />

</android.support.v4.widget.DrawerLayout>